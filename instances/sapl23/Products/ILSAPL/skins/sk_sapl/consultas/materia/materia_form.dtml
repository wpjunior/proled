     <tr> 
       <dtml-if cod_local_origem_externa>
       <td colspan="2" class="texto"><b>Local Origem: </b>
        <dtml-if cod_local_origem_externa>
          <dtml-in expr="zsql.origem_obter_zsql(cod_origem=cod_local_origem_externa)">
             <dtml-var sgl_origem> - <dtml-var nom_origem>
          </dtml-in> 
        </dtml-if>
