            &nbsp;&nbsp;&nbsp;<input type="text" name="tram_dat_tramitacao" id="tram_dat_tramitacao"size="10" maxlength="10" value="<dtml-var dat_tramitacao missing>" onBlur="formata_data(form.tram_dat_tramitacao)">
