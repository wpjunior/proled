<dtml-try>
  <dtml-call expr="zsql.unidade_tramitacao_incluir_zsql(cod_comissao = cod_comissao, 
                                                        ind_unidade_deliberativa = ind_unidade_deliberativa)">
<dtml-except>
  <dtml-let mensagem="'Não foi possível salvar a unidade de tramitação.'">
    <dtml-var mensagem_emitir>
  </dtml-let>
<dtml-else>
  <dtml-let mensagem="'Unidade de tramitação salva com sucesso.'" sucesso="1" url="'comissao_mostrar_proc?'+QUERY_STRING">
    <dtml-var mensagem_emitir>
  </dtml-let>
</dtml-try>