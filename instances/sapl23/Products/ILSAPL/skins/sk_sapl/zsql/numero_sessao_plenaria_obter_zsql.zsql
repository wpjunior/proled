<dtml-comment>
connection_id : dbcon_interlegis
arguments : 
max_rows : 1000
max_cache : 100
</dtml-comment>
<dtml-if expr="pysc.verifica_conector_bd_pysc() == 1">
SELECT REPLACE (MAX(num_sessao_plen)+1, 'L','') as novo_numero
FROM
   sessao_plenaria
   
<dtml-else>

SELECT MAX(num_sessao_plen)+1 as novo_numero
FROM
   sessao_plenaria

</dtml-if>